---
export interface Props {
	class?: string;
	size?: string;
}

const { class: className = "", size = "24" } = Astro.props;
---

<svg
	class={className}
	width={size}
	height={size}
	viewBox="0 0 24 24"
	fill="none"
	stroke="currentColor"
	stroke-width="2"
	stroke-linecap="round"
	stroke-linejoin="round"
>
	<polygon points="13,2 3,14 12,14 11,22 21,10 12,10 13,2"></polygon>
</svg>